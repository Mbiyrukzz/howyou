{
  "name": "howyou",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },

  
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#3396D3",
          "sounds": ["./assets/ringtone.mp3"]
        }
      ]
    ],
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#3396D3",
      "androidMode": "default",
      "androidCollapsedTitle": "PeepGram"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "NOTIFICATIONS"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for video calls",
        "NSMicrophoneUsageDescription": "This app uses the microphone for calls",
        "UIBackgroundModes": ["audio", "voip"]
      }
    }
  },

  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "axios": "^1.12.2",
    "expo": "~54.0.7",
    "expo-audio": "~1.0.13",
    "expo-av": "^16.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "^6.0.12",
    "expo-device": "~8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.12",
    "expo-status-bar": "~3.0.8",
    "expo-video": "~3.0.11",
    "firebase": "^12.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-config": "^1.5.9",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.0",
    "react-native-screens": "^4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-webrtc": "^124.0.6",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "ws": "^8.18.3"
  },
  "private": true
}
